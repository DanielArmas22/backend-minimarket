{
  "kind": "collectionType",
  "collectionName": "order_buys",
  "info": {
    "singularName": "order-buy",
    "pluralName": "order-buys",
    "displayName": "Order-Buy"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fechaOrden": {
      "type": "date"
    },
    "estado": {
      "type": "enumeration",
      "enum": ["pendiente", "recibida", "cancelada"],
      "default": "pendiente"
    },
    "subtotal": {
      "type": "decimal"
    },
    "igv": {
      "type": "decimal"
    },
    "total": {
      "type": "decimal"
    },
    "observaciones": {
      "type": "text"
    },
    "provider": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::provider.provider",
      "inversedBy": "order_buys"
    },
    "detail_order_buys": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::detail-order-buy.detail-order-buy",
      "mappedBy": "order_buy"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
