      "type": "text"
    },
    "approvedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "rejectionReason": {
      "type": "enumeration",
      "enum": [
        "precio",
        "proveedor",
        "cantidad",
        "prioridad",
        "presupuesto",
        "otro"
      ]
    },
    "rejectionDetails": {
      "type": "text"
    },
    "rejectedAt": {
      "type": "datetime"
    },
    "rejectedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "modificationAspects": {
      "type": "json"
    },
    "modificationDetails": {
      "type": "text"
    },
    "modificationRequestedAt": {
      "type": "datetime"
    },
    "modificationRequestedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    }
