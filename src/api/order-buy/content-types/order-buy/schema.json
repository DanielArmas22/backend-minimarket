      "type": "relation",
      "relation": "manyToOne",
      "target": "api::provider.provider",
      "inversedBy": "order_buys"
    },
    "detail_order_buys": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::detail-order-buy.detail-order-buy",
      "mappedBy": "order_buy"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
