      "enum": ["pendiente", "aprobada", "recibida", "rechazada", "cancelada", "en_revision"],
      "default": "pendiente"
    },
    "subtotal": {
      "type": "decimal"
    },
    "igv": {
      "type": "decimal"
    },
    "total": {
      "type": "decimal"
    },
    "observaciones": {
      "type": "text"
    },
