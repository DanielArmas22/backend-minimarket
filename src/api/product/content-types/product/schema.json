{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product "
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "stock": {
      "type": "integer"
    },
    "price": {
      "type": "decimal"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "mappedBy": "product"
    },
    "sales": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sale.sale",
      "mappedBy": "products"
    }
  }
}
