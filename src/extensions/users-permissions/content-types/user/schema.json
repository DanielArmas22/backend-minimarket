{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "rol": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::rol.rol",
      "inversedBy": "users_permissions_users"
    },
    "collaborator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::collaborator.collaborator",
      "inversedBy": "users_permissions_users"
    },
    "estado": {
      "type": "boolean"
    },
    "order_buys": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-buy.order-buy",
      "mappedBy": "users_permissions_user"
    },
    "sales": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sale.sale",
      "mappedBy": "users_permissions_user"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pay.pay",
      "mappedBy": "users_permissions_user"
    }
  }
}
